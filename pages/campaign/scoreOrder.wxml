<view>

<view class="main-panel">
  <view class="one-line">
    <view class="left-txt">订单号</view>
    <view class="right-txt">{{oneOrder.order_no}}</view>
  </view>

  <view class="one-line">
    <view class="left-txt">支付金额</view>
    <view class="right-txt">{{oneOrder.total}}</view>
  </view>

  <view class="one-line">
    <view class="left-txt">订单内容</view>
    <view class="right-txt">{{oneOrder.name}}</view>
  </view>
</view>


<button class="confirm-btn" open-type="getUserInfo" bindtap="prepay">确认支付</button>


</view>