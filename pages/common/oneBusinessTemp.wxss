/*.weui-media-box__hd_in-appmsg*/
page, .page{
  background-color: #eee;
}

.title{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 200rpx;
  height: 200rpx;
  margin: 5rpx;
  border-radius: 50%;
  border-color: white;
  border-style:solid;
  border-width: 2px;
  margin-top:15px;
}

.card-img{
  width: 77px;
  height: 30px;
}


.btn-card-img{
  position: absolute;
   right: 2px;
   left: auto;
   top: 10px;
  padding: 0px;
  background-color: rgba(255,255,255,0);
}

.modify-img{
  position: absolute;
   right: auto;
   left: 10px;
   top: 10px;
   width: 24px;
   height: 24px;
}

.btn-card-img::after{
  border: none;
}

.userinfo-nickname {
  color: white;
  font-size: 1em;
}

.business-job{
  margin-bottom:15px;
  color: white;
  font-size: 0.9em;
}

.business-company{
  color: white;
  font-size: 0.9em;
}

.gender{
  width: 20px;
  height: 20px;
  display: inline-block;
}


.follow-count{
  background-color: white;
  text-align: center;
}

.follow-left, .follow-middle, .follow-right{
  width: 33.33%;
  display: inline-block;
}

/*
.follow-desc, .follow-value{
  font-size: 0.7em;
}
*/

.follow-desc{
  color: rgb(195, 199, 201);
  font-size: 0.8em;
}

.business-info{
  background-color: white;
  margin-top: 10px;
}

.business-button-info{
  text-align: center;
  padding-top: 5px;
}

.business-btn-tag{
  color: black;
  font-size: 0.6em;
  width: 65px;
  height: 20px;
  display: inline-block;
  line-height: 1.8;
  vertical-align: middle;
  padding-left: 2px;
  padding-right: 2px;
  margin-right: 3px;
}

.tag1{
  background-color: rgb(128, 214, 127);
}

.tag2{
  background-color: rgb(242, 184, 121);
}

.tag3{
  background-color:rgb(137, 218, 199);
}

.default-tag, .default-tag::after{
  border: none;
}

.desc-info{
  padding-top: 5px;
  margin-left:5px;
  font-size: 0.8em;
  margin-bottom: 40px;
}

.desc-img{
  margin-right: 5px;
  width:15px; 
  height: 15px;
}

.title{
background-image:url('data:image/png;base64,');
background-repeat:no-repeat; 
background-size:100% 100%;
-moz-background-size:100% 100%;
}